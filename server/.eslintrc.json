{
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "no-unused-vars": [
        "error",
        {
          // Ignore arguments starting with _
          "argsIgnorePattern": "^_"
        }
      ]
    },
    "extends": "eslint:recommended"
  }